{
    "version": 4,
    "include": ["CMakeProjectPresets.json"],
    "configurePresets": [
        {
            "name": "mamba-dev",
            "cacheVariables": {
                "CMAKE_CXX_STANDARD": "17",
                "CMAKE_CXX_STANDARD_REQUIRED": "ON",
                "CMAKE_CXX_EXTENSIONS": "OFF",
                "BUILD_LIBMAMBA": "ON",
                "BUILD_LIBMAMBA_TESTS": "ON",
                "BUILD_LIBMAMBAPY": "ON",
                "BUILD_MICROMAMBA": "ON",
                "BUILD_MAMBA_PACKAGE": "ON",
                "BUILD_MICROMAMBA_SERVER": "ON"
            },
            "warnings": {
              "dev": false,
              "uninitialized": true
            }
        },
        {
            "name": "mamba-shared",
            "cacheVariables": {
                "BUILD_SHARED": "ON"
            }
        },
        {
            "name": "mamba-static",
            "cacheVariables": {
                "BUILD_STATIC": "ON"
            }
        },
        {
            "name": "mamba-dev-clang-shared",
            "inherits": ["mamba-dev", "mamba-shared", "conda-clang-debug"]
        },
        {
            "name": "mamba-dev-clang-static",
            "inherits": ["mamba-dev", "mamba-static", "conda-clang-debug"]
        },
        {
            "name": "mamba-dev-default-shared",
            "inherits": ["mamba-dev", "mamba-shared", "conda-default-debug"]
        },
        {
            "name": "mamba-dev-default-static",
            "inherits": ["mamba-dev", "mamba-static", "conda-default-debug"]
        }
    ]
}
